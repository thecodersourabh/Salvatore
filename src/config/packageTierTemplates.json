{
  "version": "1.0.0",
  "lastUpdated": "2025-10-31",
  
  "commonQuestions": {
    "service_title": {
      "id": "service_title",
      "label": "Package Name/Title",
      "type": "text",
      "required": true,
      "variants": {
        "Basic": {
          "placeholder": "e.g., Basic {serviceName} Package",
          "helperText": "Give your basic package a clear, descriptive name"
        },
        "Standard": {
          "placeholder": "e.g., Standard {serviceName} Package",
          "helperText": "Give your standard package a clear, descriptive name"
        },
        "Premium": {
          "placeholder": "e.g., Premium {serviceName} Package",
          "helperText": "Give your premium package a clear, descriptive name"
        }
      }
    },
    
    "package_description": {
      "id": "package_description",
      "label": "Package Description",
      "type": "textarea",
      "required": true,
      "variants": {
        "Basic": {
          "placeholder": "Describe what's included in this basic package",
          "helperText": "Be specific about what the customer will receive"
        },
        "Standard": {
          "placeholder": "Describe what's included in this standard package",
          "helperText": "Explain how this differs from the basic package"
        },
        "Premium": {
          "placeholder": "Describe what's included in this premium package",
          "helperText": "Explain how this differs from the standard package"
        }
      }
    },
    
    "price": {
      "id": "price",
      "label": "Price (₹)",
      "type": "number",
      "required": true,
      "validation": {
        "min": 0,
        "max": 10000000
      },
      "variants": {
        "Basic": {
          "placeholder": "Enter your price",
          "helperText": "Set your base price for this package"
        },
        "Standard": {
          "placeholder": "Enter your price",
          "helperText": "Set your standard price (typically higher than Basic)"
        },
        "Premium": {
          "placeholder": "Enter your price",
          "helperText": "Set your premium price (typically higher than Standard)"
        }
      }
    },
    
    "delivery_time": {
      "id": "delivery_time",
      "label": "Delivery/Completion Time ({timeUnit})",
      "type": "number",
      "required": true,
      "validation": {
        "min": 1,
        "max": 365
      },
      "placeholder": "e.g., 3 {timeUnit}",
      "helperText": "How long will this take to complete?"
    },
    
    "consultation_included": {
      "id": "consultation_included",
      "label": "Consultation/Planning Included?",
      "type": "select",
      "options": [
        "No consultation",
        "Basic phone consultation",
        "In-person consultation",
        "Virtual video consultation",
        "Both in-person and virtual"
      ],
      "required": true,
      "helperText": "Do you offer pre-service consultation?",
      "tiers": ["Premium"]
    },
    
    "rush_service_available": {
      "id": "rush_service_available",
      "label": "Rush/Express Service Available?",
      "type": "select",
      "options": [
        "No",
        "Yes, with additional charges",
        "Yes, included in package"
      ],
      "required": false,
      "helperText": "Can you provide expedited service?",
      "tiers": ["Standard", "Premium"]
    }
  },
  
